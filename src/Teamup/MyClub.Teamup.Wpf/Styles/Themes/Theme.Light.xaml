<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">

    <!--  Project  -->
    <Color x:Key="Teamup.Colors.Saved">#22CD17</Color>
    <Color x:Key="Teamup.Colors.Dirty">#E8AE07</Color>
    
    <!--  Injury  -->
    <Color x:Key="Teamup.Colors.Injury.Minor">#DA7103</Color>
    <Color x:Key="Teamup.Colors.Injury.Moderate">#D45100</Color>
    <Color x:Key="Teamup.Colors.Injury.Serious">#DE2D2D</Color>
    <Color x:Key="Teamup.Colors.Injury.Severe">#E20505</Color>
    <Color x:Key="Teamup.Colors.Injury.Muscular">#C65858</Color>
    <Color x:Key="Teamup.Colors.Injury.Fracture">#C4C9A9</Color>
    <Color x:Key="Teamup.Colors.Injury.Trauma">#7D5A4A</Color>
    <Color x:Key="Teamup.Colors.Injury.Ligament">#5005E2</Color>
    <Color x:Key="Teamup.Colors.Injury.Sickness">#A068DE</Color>

    <!--  License state  -->
    <Color x:Key="Teamup.Colors.License.Unknown">#828282</Color>
    <Color x:Key="Teamup.Colors.License.Given">#A1260D</Color>
    <Color x:Key="Teamup.Colors.License.Back">#DD6E00</Color>
    <Color x:Key="Teamup.Colors.License.Paid">#388A34</Color>

    <!--  Absence  -->
    <Color x:Key="Teamup.Colors.Absence">#A1260D</Color>

    <!--  Attendance  -->
    <Color x:Key="Teamup.Colors.Attendance.InHolidays">#3D1D78</Color>
    <Color x:Key="Teamup.Colors.Attendance.InSelection">#CE9F05</Color>
    <Color x:Key="Teamup.Colors.Attendance.Resting">#0B73A6</Color>
    <Color x:Key="Teamup.Colors.Attendance.Injured">#712F08</Color>

    <!--  Position  -->
    <Color x:Key="Teamup.Colors.Position.Inefficient">#D40000</Color>
    <Color x:Key="Teamup.Colors.Position.Poor">#D45100</Color>
    <Color x:Key="Teamup.Colors.Position.Average">#CE9F05</Color>
    <Color x:Key="Teamup.Colors.Position.Good">#809C11</Color>
    <Color x:Key="Teamup.Colors.Position.VeryGood">#1A4C0C</Color>
    <Color x:Key="Teamup.Colors.Position.Natural">#2CB407</Color>

    <!--  Rating  -->
    <Color x:Key="Teamup.Colors.Rating.0To2">#D40000</Color>
    <Color x:Key="Teamup.Colors.Rating.2To35">#CE3B3B</Color>
    <Color x:Key="Teamup.Colors.Rating.35To45">#D45100</Color>
    <Color x:Key="Teamup.Colors.Rating.45To6">#CE9F05</Color>
    <Color x:Key="Teamup.Colors.Rating.6To8">#1A4C0C</Color>
    <Color x:Key="Teamup.Colors.Rating.8To10">#2CB407</Color>

    <!--  State  -->
    <Color x:Key="Teamup.Colors.Email.Draft">#5D31D1</Color>

    <!-- Tactic -->
    <Color x:Key="Teamup.Colors.Ground">#4F8306</Color>
    <Color x:Key="Teamup.Colors.Ground.Dark">#40650C</Color>

    <!-- Competition -->
    <Color x:Key="Teamup.Colors.Competition.Friendly">#8E8D8D</Color>
    <Color x:Key="Teamup.Colors.Competition.League">#4E2FAA</Color>
    <Color x:Key="Teamup.Colors.Competition.Cup">#30A032</Color>

    <!-- Match -->
    <Color x:Key="Teamup.Colors.Match.State.Suspended">#CE3B3B</Color>

    <SolidColorBrush x:Key="Teamup.Brushes.Saved" po:Freeze="true" Color="{StaticResource Teamup.Colors.Saved}" />
    <SolidColorBrush x:Key="Teamup.Brushes.Dirty" po:Freeze="true" Color="{StaticResource Teamup.Colors.Dirty}" />
    <SolidColorBrush x:Key="Teamup.Brushes.Injury.Minor" po:Freeze="true" Color="{StaticResource Teamup.Colors.Injury.Minor}" />
